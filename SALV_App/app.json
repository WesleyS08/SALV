{
  "expo": {
    "name": "SALV_App_",
    "slug": "SALV_App_",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#0D283D"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#0D283D"
      },
      "permissions": [
        "NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.wesleydavi.salv"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/img/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D283D"
        }
      ],
      "expo-secure-store",
      [
        "expo-background-fetch",
        {
          "minimumInterval": 15,
          "stopOnTerminate": false,
          "startOnBoot": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c100e024-e62d-4e82-872e-92e1491eccac"
      }
    },
    "owner": "salv14365435g"
  }
}
